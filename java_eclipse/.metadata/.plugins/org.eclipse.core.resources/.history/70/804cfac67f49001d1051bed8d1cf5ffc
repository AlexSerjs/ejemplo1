package es.iesclaradelrey.dm1e.primerTrimestre.ut4.ejercicios.condicionalesJava;

import javax.swing.JOptionPane;

public class Ejercicio1_Condicionales {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//JOptionPane.showInputDialog(null, "ESCRIBE DOS NUMEROS A CONTINUACION");
		
		String stringNum=JOptionPane.showInputDialog("Primer Nº");
		int num = Integer.parseInt(stringNum);
		
        String stringNum1=JOptionPane.showInputDialog("Segundo  Nº");
		int num1 = Integer.parseInt(stringNum1);
		
		if (num<num1) {
			
			JOptionPane.showInputDialog("El segundo numero es mayor");
		}
			else if (num>num1) {
				JOptionPane.showInputDialog("El primero numero es mayor");
				
			} 
			else {
				JOptionPane.showInputDialog("Son iguales ");

			}
			
		}
		
		

	}

}
