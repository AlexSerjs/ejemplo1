package es.iesclaradelrey.dm1e.primerTrimestre.ut4.ejercicios.basicos;

import java.util.Scanner;

public class Ej10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		System.out.println("Escoge un numero \nMenu");
		System.out.println("1.- Todos los Numeros\n2.- Impares\n3.- Pares\n0.- Salir");
		int num = sc.nextInt();

		switch (num) {
		case 1: for (int i = 1; i <= 200; i++) {
			
			System.out.print(i+" | ");
		}
			

			break;
		case 2:
			for (int i = 1; i <=200; i+=2) {
				
				System.out.print(i+" | ");
			}

			break;
		case 3:

			break;

		default:
			if (num<=0 && num >3) {
				System.out.println("Adios");
			}
			break;
		}

		sc.close();
	}

}
